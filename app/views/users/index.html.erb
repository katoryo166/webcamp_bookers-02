<p id="notice"></p>
<div class="container">
  <div class="row">
    <div class="col-md-3">
      <%= render 'users/user', user:@user %>
      <%= render 'books/book', book:@book %>
    </div>
    <h2>Users</h2>
        <table>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= attachment_image_tag user, :profile_image, fallback: "no_image.jpg", size: "60x60"%></td>
              <td><%= user.name %></td>
              <td><%= link_to "show", user_path(user.id) %></td>
            </tr>
          <% end %>
        </tbody>
        </table>
      </div>
      </div>